apiVersion: apps/v1
kind: Deployment
metadata:
  annotations: null
  labels:
    run: pycrudapi
  name: pycrudapi
  namespace: python-crud-api
spec:
  replicas: 1
  selector:
    matchLabels:
      run: pycrudapi
  template:
    metadata:
      labels:
        run: pycrudapi
    spec:
      containers:
      - image: ghcr.io/longwuyuan/python-crud-api:master-7890a83
        imagePullPolicy: IfNotPresent
        name: pycrudapi
        ports:
        - containerPort: 5000
          protocol: TCP
        resources: {}
      restartPolicy: Always
